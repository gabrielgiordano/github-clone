<%= render '/projects/project_header' %>

<%= render :layout => '/projects/project_panel', :locals => {:menu => { add_file: true } } do %>

  <%= form_tag(project_files_path, method: "post", multipart: true) do  %>
    <div class="form-group">
      <%= label_tag(:file, "File to Upload:") %>
      <%= file_field_tag :file %>
    </div>

    <%= submit_tag("Upload", class: "btn btn-success") %>
  <% end %>

<% end %>
