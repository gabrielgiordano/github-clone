<table>
  <thead>
    <tr>
      <th>FileName</th>
      <th>Last Modification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% files.each do |file| %>
      <tr>
        <td><%= link_to file[:name], project_suggestion_file_path(project_id, suggestion_id, file[:name]) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
